    "cssnano": "^6.0.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "postcss-cli": "^11.0.0",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.3.6"
